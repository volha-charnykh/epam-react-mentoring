{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "babel-plugin-dynamic-import-node",
    "react-loadable/babel",
    "@babel/plugin-syntax-throw-expressions",
    "@babel/plugin-proposal-throw-expressions",
    ["@babel/plugin-transform-runtime", {
      "regenerator": true
    }]
  ]
}
